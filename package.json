{
  "name": "sgframework-highlighter",
  "displayName": "SGFramework Syntax-highlighter",
  "description": "Syntax Highlighter for SGFramework",
  "author": {"name": "Jonas Bodingbauer", "email": "e11802486@student.tuwien.ac.at"},
  "repository": {"type": "git",
                  "url": "https://github.com/Caesiumhydroxid/VSCode-SGFramework-SyntaxHighlighting"},
  "version": "0.0.3",
  "publisher": "caesiumhydroxid",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "sgframework",
      "aliases": ["SG Framework", "sgframework"],
      "extensions": [".sg", ".sk"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "sgframework",
      "scopeName": "source.sg",
      "path": "./syntaxes/sgframework.tmLanguage.json"
    }]
  }
}
